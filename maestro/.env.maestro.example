# Maestro Test Environment Variables
# Copy this file to .env.maestro and fill in your test credentials

# Test User Credentials
TEST_EMAIL=your_test_email@example.com
TEST_PASSWORD=your_test_password

# Alternative Test Users (for multi-user scenarios)
TEST_EMAIL_2=second_test_email@example.com
TEST_PASSWORD_2=second_test_password

# Admin User (for admin-specific tests)
ADMIN_EMAIL=admin_test@example.com
ADMIN_PASSWORD=admin_test_password

# Test Data
TEST_CONTACT_NAME=Test Contact
TEST_CONTACT_EMAIL=contact@example.com
TEST_CONTACT_PHONE=+1234567890

# Itinerary Test Data
TEST_ITINERARY_NAME=Test Itinerary
TEST_PASSENGER_NAME=John Doe
TEST_PASSENGER_EMAIL=passenger@example.com

# Environment Configuration
# For Staging use: https://wrgkiastpqituocblopg.supabase.co
# For Local use: http://localhost:54321
TEST_BASE_URL=https://wrgkiastpqituocblopg.supabase.co
TEST_TIMEOUT=30000

# Feature Flags (optional)
ENABLE_SCREENSHOTS=true
ENABLE_VIDEO_RECORDING=true
VIDEO_QUALITY=high
VIDEO_SAVE_ON_SUCCESS=false
VIDEO_SAVE_ON_FAILURE=true
VERBOSE_LOGGING=false